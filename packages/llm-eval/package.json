{
  "name": "jest-llm-eval",
  "version": "2.0.0-beta.1",
  "type": "module",
  "description": "AI-powered evaluation framework for testing LLM responses with Jest",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jest-llm-eval": "./dist/cli-viewer.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./terminal-reporter": {
      "import": "./dist/terminal-reporter.js",
      "require": "./dist/terminal-reporter.js",
      "types": "./dist/terminal-reporter.d.ts"
    },
    "./evaluation-reporter": {
      "import": "./dist/evaluation-reporter.js",
      "require": "./dist/evaluation-reporter.js",
      "types": "./dist/evaluation-reporter.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "jest",
    "testing",
    "llm",
    "ai",
    "evaluation",
    "language-model",
    "test-framework",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/jest-llm-eval#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/jest-llm-eval.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/jest-llm-eval/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "@jest/globals": "^29.0.0",
    "jest": "^29.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^2.0.0-beta.5",
    "@ai-sdk/react": "^2.0.0-beta.7",
    "ai": "^5.0.0-beta.7",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.3",
    "figlet": "^1.7.0",
    "ora": "^8.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/types": "^30.0.1",
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "8.35.1",
    "@typescript-eslint/parser": "8.35.1",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  }
}
